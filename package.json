{
  "name": "quickcredit",
  "version": "1.0.0",
  "description": "This is web app that grants people loan in other to allieviate poverty.",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=production&& npm run migrate&& --timeout 50000 babel-node -- server/app.js",
    "start:dev": "nodemon --watch server --exec babel-node -- server/app.js",
    "postinstall": "babel server -d build",
    "test": "set NODE_ENV=test&& npm run migrate&& nyc --reporter=html --reporter=text mocha --timeout 50000 --exit --require @babel/register ./server/spec/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint server/",
    "heroku-postbuild": "npm run migrate",
    "migrate": "babel-node -- server/models/tableSchema"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Morsetim/QuickCredit.git"
  },
  "author": "Maurice Etim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Morsetim/QuickCredit/issues"
  },
  "homepage": "https://github.com/Morsetim/QuickCredit#readme",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "debug": "^4.3.1",
    "dotenv": "^6.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^1.16.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mocha": "^5.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^1.19.4",
    "nyc": "^13.3.0",
    "pg": "^7.18.2",
    "supertest": "^3.4.2",
    "swagger-jsdoc": "^3.7.0",
    "validator": "^10.11.0"
  }
}
